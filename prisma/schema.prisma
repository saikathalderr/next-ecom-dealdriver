generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Product {
    id                 Int      @id
    title              String
    description        String
    price              Int
    discountPercentage Float
    rating             Float
    stock              Int
    brand              String
    category           String
    thumbnail          String
    images             String[]
}
